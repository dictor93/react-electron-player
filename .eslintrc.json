{
  "env": {
    "node": true,
    "browser": true,
    "commonjs": true,
    "es6": true,
    "es2021": true
  },
  "extends": [
    "eslint:recommended",
    "plugin:react/recommended"
  ],
  "parser": "babel-eslint",
  "parserOptions": {
    "sourceType": "module",
    "allowImportExportEverywhere": true,
    "ecmaFeatures": {
      "jsx": true
    },
    "ecmaVersion": 12
  },
  "plugins": [
    "react"
  ],
  "rules": {
    "max-len": [
      "warn",
      {
        "code": 130,
        "ignoreStrings": true
      }
    ],
    "no-unreachable": 1,
    "arrow-body-style": 1,
    "no-unused-vars": "warn",
    "jsx-quotes": [
      "error",
      "prefer-double"
    ],
    "react/jsx-filename-extension": [
      2,
      {
        "extensions": [
          ".js"
        ]
      }
    ],
    "arrow-parens": [
      "error",
      "as-needed"
    ],
    "semi": [
      2,
      "never"
    ],
    "camelcase": 0,
    "prefer-destructuring": 0,
    "no-underscore-dangle": 0,
    "no-restricted-imports": 0,
    "import/prefer-default-export": 0,
    "no-shadow": 0,
    "linebreak-style": 0,
    "no-param-reassign": 0,
    "consistent-return": 0,
    "react/no-danger": 0,
    "react/forbid-prop-types": 0,
    "react/no-array-index-key": 0,
    "react/jsx-one-expression-per-line": 0,
    "jsx-a11y/click-events-have-key-events": 0,
    "jsx-a11y/no-static-element-interactions": 0,
    "jsx-a11y/no-noninteractive-element-interactions": 0,
    "react/jsx-props-no-spreading": 0,
    "react/prop-types": 1,
    "react/destructuring-assignment": "warn",
    "no-var": "error",
    "prefer-const": "error",
    "react/jsx-key": "warn"
  }
}